{
  "name": "Mediusflow Chrome Extension",
  "version": "2.0",
  "manifest_version": 2,
  "options_page": "options.html",
  "description": "This is a Chrome extension for Mediusflow.",
  "permissions": [
	"tabs",
	"http://www.w3.org/2003/05/soap-envelope",
	"http://www.w3.org/2003/05/soap-encoding",
	"http://www.w3.org/2001/XMLSchema-instance",
	"http://www.w3.org/2001/XMLSchema",
	"http://schemas.datacontract.org/2004/07/Medius.Gemini.ClientModel",
	"http://schemas.microsoft.com/2003/10/Serialization/",
	"http://schemas.microsoft.com/2003/10/Serialization/Arrays",
	"http://tempuri.org/",
	"https://*.mediusflow.com/*"
  ],
  "background":{
  	"scripts": ["jquery.js", "eventPage.js"]
   },
  "browser_action": {
	"name": "Mediusflow Chrome Extension",
    	"default_icon": "mediuscircle.png"
  }
}